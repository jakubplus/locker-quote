{
  "name": "app/locker-quote",
  "description": "Micro REST API for locker size and price quotes",
  "type": "project",
  "require": {
    "php": "^8.3",
    "symfony/dotenv": "^7.1",
    "symfony/framework-bundle": "^7.1",
    "symfony/http-foundation": "^7.1",
    "symfony/http-kernel": "^7.1",
    "symfony/routing": "^7.1",
    "symfony/dependency-injection": "^7.1",
    "symfony/config": "^7.1",
    "symfony/yaml": "^7.3",
    "symfony/twig-bundle": "^7.1",
    "twig/twig": "^3.0",
    "symfony/string": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8000 -t public"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^7.1",
    "symfony/browser-kit": "^7.1",
    "symfony/css-selector": "^7.1"
  }
}
